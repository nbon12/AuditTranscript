        "team": "T026NJJ6Z"
    },
    {
        "client_msg_id": "2033ddb6-da89-49c0-b6fe-111db24ae8e7",
        "type": "message",
        "text": "yes",
        "user": "UMQA682PJ",
        "ts": "1567611592.008600",
        "team": "T026NJJ6Z"
    },
    {
        "client_msg_id": "bd5124b7-6123-41a8-b9f3-36ec5db52d72",
